{
  "name": "@jade/shared-types",
  "version": "0.1.0",
  "description": "Shared TypeScript types for JADE Spa Marketplace",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./graphql": {
      "types": "./dist/graphql/generated.d.ts",
      "require": "./dist/graphql/generated.js",
      "import": "./dist/graphql/generated.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist src/graphql/generated.ts"
  },
  "devDependencies": {
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "graphql": "^16.8.1",
    "zod": "^3.22.4"
  }
}
